extends header

block content
    div(class="list-group")
        each hit, index in data.hits
            a(class="list-group-item clearfix" href="/music/queue?#{hit._id}")
                div(class="row")
                    div(class="col-xs-8")
                        h3(class="title") #{hit.title}
                        h4(class="artist") #{hit.artist}
                        p(class="album list-group-item-text") #{hit.album}
                    div(class="col-xs-4")
                        span(class="pull-right glyphicon glyphicon-ok" style="font-size:1.5em;")


