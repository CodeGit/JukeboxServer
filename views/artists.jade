extends header
block content
    div(class="list-group")
        each hit, index in data.hits
            a(class="list-group-item clearfix" href="/music/filter?artist=#{hit._id}")
                p(class="title") #{hit.artist}
                span(class="badge") #{hit.music}
                //span(class="pull-right glyphicon glyphicon-ok" style="font-size:1.5em;")
            